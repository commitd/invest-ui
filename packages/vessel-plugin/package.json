  "jest": {
    "transform": {
      "^.+\\.tsx?$": "../../node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
