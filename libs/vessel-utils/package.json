{
  "name": "invest-utils",
  "version": "0.1.0",
  "module": "src/index.ts",
  "main": "build/index.js",
  "browser": "build/index.js",
  "types": "build/index.d.ts",
  "license": "Apache-2.0",
  "dependencies": {
    "loglevel": "^1.5.1",
    "loglevel-plugin-prefix": "^0.5.3",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/loglevel": "^1.5.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.4",
    "tslint": "^5.7.0",
    "typescript": "~2.5.0"
  },
  "peerDependencies": {
    "invest-types": "*"
  },
  "scripts": {
    "build": "tsc",
    "build:systemjs": "tsc -p tsconfig.systemjs.json",
    "copy:systemjs": "cp -a lib/. ../../build/.",
    "watch": "tsc -w",
    "test": "jest",
    "publish:local": "npm unpublish --registry http://localhost:4873 $npm_package_name@$npm_package_version && yarn publish --registry http://localhost:4873 --new-version $npm_package_version"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "../../node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "build/",
      "lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}